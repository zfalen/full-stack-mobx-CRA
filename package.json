{
  "name": "blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production nf start -p $PORT",
    "yaaamon": "NODE_ENV=dev nodemon ./bin/www",
    "test": "NODE_ENV=test mocha --no-deprecation --sort --colors --inline-diffs --bail",
    "dev": "nf start -p 3000 --procfile Procfile.dev",
    "setupServer": "rm -rf node_modules && yarn install",
    "setupClient": "cd client && rm -rf node_modules && yarn install",
    "setup": "npm run setupServer && npm run setupClient && npm run dev"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "colors": "^1.1.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "dotenv": "^4.0.0",
    "express": "~4.13.1",
    "foreman": "^2.0.0",
    "mongoose": "^4.9.8",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  }
}
